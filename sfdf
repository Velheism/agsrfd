local UserInputService = game:GetService("UserInputService").InputBegan:Connect(function(input, chat)
    if not chat then
    if input.KeyCode == Enum.KeyCode.V then
            for i, v in pairs(game:FindFirstChildWhichIsA("Workspace").Camera["Safe Zone"]:GetChildren()) do 
                if v.Name == "Part" then
                    v.Transparency = 0.8
                end
            end
    end
    end
end)



local UserInputService = game:GetService("UserInputService").InputBegan:Connect(function(input, chat)
    if not chat then
    if input.KeyCode == Enum.KeyCode.G then
            for i, v in pairs(game:FindFirstChildWhichIsA("Workspace").Camera["Safe Zone"]:GetChildren()) do 
                if v.Name == "Part" then
                    v.Transparency = 1
                end
            end
    end
    end
end)
